<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="410.0" prefWidth="406.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ProductController">
    <children>
        <TableView fx:id="tableview_products" onMouseClicked="#Clicked" layoutX="33.0" layoutY="14.0" prefHeight="303.0" prefWidth="336.0">
            <columns>
                <TableColumn fx:id="column_id" prefWidth="37.0" text="ID" />
                <TableColumn fx:id="column_name" prefWidth="150.0" text="Название" />
                <TableColumn fx:id="column_type" prefWidth="148.0" text="Тип" />
            </columns>
        </TableView>
        <Button fx:id="button_add" layoutX="33.0" layoutY="335.0" mnemonicParsing="false" onAction="#Add" text="Добавить" />
        <Button fx:id="button_delete" disable="true" layoutX="120.0" layoutY="335.0" mnemonicParsing="false" onAction="#Delete" text="Удалить" />
        <Button fx:id="button_edit" disable="true" layoutX="201.0" layoutY="335.0" mnemonicParsing="false" onAction="#Edit" text="Редактировать" />
        <Button fx:id="button_back" layoutX="320.0" layoutY="335.0" mnemonicParsing="false" onAction="#Back" text="Назад" />
    </children>
</AnchorPane>
