<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="379.0" prefWidth="606.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.UserController">
   <children>
      <TableView fx:id="tableview_users" onMouseClicked="#Clicked" layoutX="33.0" layoutY="14.0" prefHeight="310.0" prefWidth="531.0">
         <columns>
            <TableColumn fx:id="column_id" prefWidth="37.0" text="ID" />
            <TableColumn fx:id="column_login" prefWidth="127.0" text="Логин" />
            <TableColumn fx:id="column_password" prefWidth="136.0" text="Пароль"/>
            <TableColumn fx:id="column_name" prefWidth="113.0" text="Имя" />
            <TableColumn fx:id="column_role" prefWidth="117.0" text="Роль" />
         </columns>
      </TableView>
      <Button fx:id="button_add" onAction="#Add" layoutX="33.0" layoutY="335.0" mnemonicParsing="false" text="Добавить" />
      <Button fx:id="button_delete" onAction="#Delete" disable="true" layoutX="131.0" layoutY="335.0" mnemonicParsing="false" text="Удалить" />
      <Button fx:id="button_edit" onAction="#Edit" disable="true" layoutX="218.0" layoutY="335.0" mnemonicParsing="false" text="Редактировать" />
      <Button fx:id="button_back" onAction="#Back" layoutX="512.0" layoutY="335.0" mnemonicParsing="false" text="Назад" />
   </children>
</AnchorPane>
